{% for report in reports %}
<tr>
    <td>{{report.id }}</td>
    <td>{{report.date }}</td>
    <td>
        <button type="button" class="btn btn-info btn-xs">Modifier</button>
        <button type="button" class="btn btn-danger btn-xs delete-element" id="element-{{report.id }}" value="{{report.id }}">Supprimer</button>   
    </td>
</tr>
{% else %}
<tr class="danger">
    <td colspan="3">{{'No_report'|trans }}</td>
</tr>
{% endfor %}